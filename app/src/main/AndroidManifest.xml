<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nubes.myclouds">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="nubes.atlas.myclouds.activity.QuizActivity"
            android:label="@string/title_activity_quiz"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="nubes.atlas.myclouds.activity.DicotomicaActivity"
            android:label="@string/title_activity_dicotomica"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="nubes.atlas.myclouds.activity.NuvolDataActivity"
            android:label="@string/title_activity_nuvol_data"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="nubes.atlas.myclouds.activity.NuvolListActivity"
            android:label="@string/title_activity_nuvol_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="nubes.atlas.myclouds.activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>